{
  "name": "papago-sdk-js",
  "version": "0.0.2",
  "description": "Papago SDK for Node.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">= 14"
  },
  "scripts": {
    "build": "bunchee",
    "test": "jest --env node",
    "clean": "rm -rf ./dist",
    "prepublishOnly": "pnpm clean && pnpm build && pnpm test"
  },
  "dependencies": {
    "@types/node-fetch": "^2.6.4",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "@swc/jest": "^0.2.29",
    "@types/jest": "^29.5.2",
    "bunchee": "^3.5.1",
    "dotenv": "^16.3.1",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules"
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    }
  },
  "files": [
    "dist",
    "*.md"
  ],
  "packageManager": "pnpm@8.6.12",
  "repository": {
    "type": "git",
    "url": "https: //github.com/devjiwonchoi/papago-sdk"
  },
  "author": "Jiwon Choi",
  "license": "MIT",
  "keywords": [
    "papago",
    "translate",
    "translator",
    "sdk"
  ]
}