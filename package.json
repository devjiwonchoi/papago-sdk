{
  "name": "papago-sdk-js",
  "version": "0.1.0",
  "description": "Papago SDK for Node.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "bunchee",
    "test": "jest --env node",
    "clean": "rm -rf ./dist",
    "prepublishOnly": "pnpm clean && pnpm build && pnpm test"
  },
  "dependencies": {
    "@types/node-fetch": "^2.6.4",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "@swc/jest": "^0.2.29",
    "@types/jest": "^29.5.2",
    "bunchee": "^3.5.1",
    "dotenv": "^16.3.1",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules"
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    }
  },
  "files": [
    "dist",
    "*.md"
  ],
  "repository": "https://github.com/devjiwonchoi/papago-sdk.git",
  "author": "Jiwon Choi @devjiwonchoi",
  "license": "MIT",
  "keywords": [
    "papago",
    "api",
    "sdk",
    "client",
    "papago-sdk",
    "papago-sdk-js",
    "translate",
    "translator"
  ]
}